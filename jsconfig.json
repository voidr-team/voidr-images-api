{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#src/*": ["src/*"],
      "#routes/*": ["src/routes/*"],
      "#models/*": ["src/infra/models/*"],
      "#repositories/*": ["src/infra/repositories/*"],
      "#providers/*": ["src/infra/providers/*"],
      "#domain/*": ["src/domain/*"],
